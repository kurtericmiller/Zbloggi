CREATE TABLE `sections` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `published` tinyint(1) NOT NULL DEFAULT '0',
  `section_title` varchar(150) NOT NULL,
  `section_text` text NOT NULL,
